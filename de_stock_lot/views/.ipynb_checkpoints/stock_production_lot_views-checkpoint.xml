<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_production_lot_form_inherit_oem_serial" model="ir.ui.view">
        <field name="name">view.production.lot.form.inherit.oem.serial</field>
        <field name="model">stock.production.lot</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <field name="ref" position="after">
                    <field name="oem_serial_no" />
            </field>
        </field>
      </record>
    
    <record id="view_production_lot_tree_inherit_oem_serial" model="ir.ui.view">
        <field name="name">view.production.lot.tree.inherit.oem.serial</field>
        <field name="model">stock.production.lot</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="stock.view_production_lot_tree"/>
        <field name="arch" type="xml">
            <field name="ref" position="after">
                    <field name="oem_serial_no" />
            </field>
        </field>
      </record>
    
</odoo>