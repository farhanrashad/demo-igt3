<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
        <record id="view_travel_request_seq" model="ir.sequence">
            <field name="name">travel.request.travel_request</field>
            <field name="code">travel.request.travel_request</field>
            <field name="prefix">TR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="id" eval="False"/>
        </record>
        <record id="view_ticket_allocation_seq" model="ir.sequence">
            <field name="name">ticket.allocation.seq</field>
            <field name="code">ticket.allocation.seq</field>
            <field name="prefix">TI/%(year)s/</field>
            <field name="padding">5</field>
            <field name="id" eval="False"/>
        </record>
        <record id="view_ticket_balance_seq" model="ir.sequence">
            <field name="name">ticket.balance.seq</field>
            <field name="code">ticket.balance.seq</field>
            <field name="prefix">TB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="id" eval="False"/>
        </record>
    </data>
</odoo>
