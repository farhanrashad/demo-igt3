<odoo>
    <data>
        
        <record id="view_account_custom_entry_import" model="ir.ui.view">
            <field name="name">account.custom.entry.import</field>
            <field name="model">account.custom.entry</field>
            <field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='custom_entry_type_id']" position="after">
                    <field name="is_custom_entry_import"   invisible="1"/>
                   <field name="entry_attachment_id"  widget="many2many_binary" invisible="1" ></field>
                </xpath>
                <xpath expr="//field[@name='custom_entry_line']//tree//field[@name='amount_advanced']" position="after">
			<field name="line_vals"/>
		</xpath>
              
           </field>
        </record>


       

       
    </data>
</odoo>