<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_custom_entry_form_view_inherit_penalty" model="ir.ui.view">
            <field name="name">account.custom.entry.form.view.inehrit.penalty</field>
            <field name="model">account.custom.entry</field>
            <field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//sheet[1]" position="inside">
                    <field name="has_om_fields" invisible="1"/>
                </xpath>
                <xpath expr="//page[@name='custom_entry_line']/field[@name='custom_entry_line']/tree[1]" position="inside">
                    <field name="o_tower_type" string="Tower Type" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_product_id" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_date_rfi" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_date_onair" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_date_handover" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_date_start" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_date_end" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_days_rfi" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_days_onair" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_amount" string="Amount" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_final_amount" string="Final Amount" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    <field name="o_charges" string= "Service Charges" attrs="{'invisible':[('parent.has_om_fields','==','no')], 'required': [('parent.has_om_fields','==','required')]}"/>
                    
                </xpath>
                
            </field>
    </record>
</odoo>