<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_custom_entry_form_view_inherit_om" model="ir.ui.view">
            <field name="name">account.custom.entry.form.view.inehrit.om</field>
            <field name="model">account.custom.entry</field>
            <field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//sheet[1]" position="inside">
                    <field name="has_fuel_drawn" invisible="1"/>
                    <field name="has_fuel_filling" invisible="1"/>
                </xpath>
                <xpath expr="//page[@name='custom_entry_line']/field[@name='custom_entry_line']/tree[1]" position="inside">
                    <!-- Fuel drawn-->
                    <field name="d_date" string="Date" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_partner_id" string="Purchase Partner" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_contact_id" string="Purchase From" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_product_qty" string="Fuel Qty" sum="Drawn Product Qty" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_price_unit" string="Unit Price" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_price_subtotal" string="Subtotal" sum="Drawn Total" attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_booklet_no" string="Booklet No." attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                    <field name="d_receipt_no" string="Receipt No." attrs="{'column_invisible':[('parent.has_fuel_drawn','==','no')], 'required': [('parent.has_fuel_drawn','==','required')]}"/>
                                    
                    <!-- Fuel Filled-->
                    <field name="f_date" string="Date" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_partner_id" string="Purchase Contact" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_contact_id" string="Purchase From" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_gen_name" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_gen_capacity" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_curr_drgh" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_opening_stock" sum="Filled Opening Stock" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_closing_stock" sum="Filled Closing Stock" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_product_qty" string="Fuel Qty" sum="Filled Qty" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_price_unit" string="Unit Price" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_price_subtotal" string="Subtotal" sum="Filled Total" attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_booklet_no" string="Booklet No." attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                    <field name="f_receipt_no" string="Receipt No." attrs="{'column_invisible':[('parent.has_fuel_filling','==','no')], 'required': [('parent.has_fuel_filling','==','required')]}"/>
                </xpath>
                
            </field>
    </record>
</odoo>