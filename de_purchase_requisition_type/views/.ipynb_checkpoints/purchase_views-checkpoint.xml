<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--Purchase Order -->
        <record id="purchase_order_form_inherit_approvals" model="ir.ui.view">
		<field name="name">purchase.order.form.inherit.approvals</field>
		<field name="model">purchase.order</field>
		<field name="inherit_id" ref="purchase.purchase_order_form" />
		<field name="arch" type="xml">
			<xpath expr="//group[1]/group[2]" position="inside">
                <field name="requisition_type_id" string="Order Type" required="1" options="{'no_create':True,'no_open':True}"/> 
			</xpath>
		</field>
	</record>
        
    </data>
</odoo>