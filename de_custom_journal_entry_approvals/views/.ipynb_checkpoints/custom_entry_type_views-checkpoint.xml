<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Custom Entry Type -->
        <record id="view_custom_entry_type_form_inherit_approvals" model="ir.ui.view">
			<field name="name">view.custom.entry.type.form.inherit.approvals</field>
			<field name="model">account.custom.entry.type</field>
			<field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_type_form_view"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='journal_id']" position="after">
                    <field name="category_id" string="Approval Category"/>
				</xpath>
			</field>
		</record>
    </data>
</odoo>