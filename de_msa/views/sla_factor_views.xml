<odoo>

	<record id="view_sla_factor_form" model="ir.ui.view">
		<field name="name">sla.factor.value.form</field>
		<field name="model">sla.factor.value</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					
                    <div class="oe_title">
                        <h1>
                            <group>
                                <field name="name" placeholder="SLA Factor One"/>
                            </group>
                        </h1>
                    </div>
					
				</sheet>
			</form>
		</field>
	</record>

	<record id="view_sla_factor_tree" model="ir.ui.view">
		<field name="name">sla.factor.value.tree</field>
		<field name="model">sla.factor.value</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
			</tree>
		</field>
	</record>

	<record id="view_sla_factor_search" model="ir.ui.view">
		<field name="name">sla.factor.value.search</field>
		<field name="model">sla.factor.value</field>
		<field name="arch" type="xml">
			<search>
				<field name="name"/>
			</search>
		</field>
	</record>


	<record id="action_sla_factor_view" model="ir.actions.act_window">
		<field name="name">SLA Factors</field>
		<field name="res_model">sla.factor.value</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree,form</field>
		<field name="domain">[]</field>
		<field name="context">{}</field>
		<field name="search_view_id" ref="view_sla_factor_search" />
		<field name="view_id" ref="view_sla_factor_tree" />
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Create SLA Factors Here...
			</p>
		</field>
	</record>

</odoo>