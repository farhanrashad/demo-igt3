<odoo>

	<record id="view_monthly_power_model_tree" model="ir.ui.view">
		<field name="name">monthly.power.model.tree</field>
		<field name="model">monthly.power.model</field>
		<field name="arch" type="xml">
			<tree editable="bottom">
				<field name="msa_id"/>
				<field name="site_id"/>
				<field name="power_model"/>
				<field name="period"/>
			</tree>
		</field>
	</record>

	<record id="view_monthly_power_model_seach" model="ir.ui.view">
		<field name="name">monthly.power.model.search</field>
		<field name="model">monthly.power.model</field>
		<field name="arch" type="xml">
			<search>
				<field name="msa_id"/>
				<field name="site_id"/>
				<field name="power_model"/>
				<field name="period"/>
			</search>
		</field>
	</record>


	<record id="action_monthly_power_model_view" model="ir.actions.act_window">
		<field name="name">Monthly Power Model</field>
		<field name="res_model">monthly.power.model</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree,form</field>
		<field name="domain">[]</field>
		<field name="context">{}</field>
		<field name="search_view_id" ref="view_monthly_power_model_seach" />
		<field name="view_id" ref="view_monthly_power_model_tree" />
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Create Monthly Power Models Here...
			</p>
		</field>
	</record>





