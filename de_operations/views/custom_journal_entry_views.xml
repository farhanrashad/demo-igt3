<odoo>
    <data>
        <!-- custom entries -->
        <record id="action_operations_custom_entry" model="ir.actions.act_window">
            <field name="name">Dashboard</field>
            <field name="res_model">account.custom.entry.type</field>
            <field name="view_mode">kanban</field>
            <field name="domain">[('move_type', 'in', ['entry','out_invoice','in_invoice','out_receipt','in_receipt'])]</field>
        </record>
        
    <menuitem action="action_operations_custom_entry" name="Custom Entry" id="menu_operations_custom_entry_custom_entry" groups="account.group_account_manager" parent="operations_custom_entries_menu" sequence="1"/>
        
        <!-- Custom penalties -->
         <record id="action_operations_custom_entry_penalty" model="ir.actions.act_window">
            <field name="name">Dashboard</field>
            <field name="res_model">account.custom.entry.type</field>
            <field name="view_mode">kanban</field>
            <field name="domain">[('move_type', 'in', ['out_refund','in_refund',])]</field>
        </record>
        <menuitem action="action_operations_custom_entry_penalty" name="Penalty" id="menu_operations_penalty_custom_entry" groups="account.group_account_manager" parent="operations_custom_entries_menu" sequence="2"/>

        
        
    </data>
</odoo>