<odoo>
    <data>

        <record id="view_hr_payslip_form_inherit_overtime" model="ir.ui.view">
            <field name="name">view.hr.payslip.form.inehrit.overtime</field>
            <field name="model">hr.payslip</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="overtime_ids" invisible="1"/>
                </field>
                <field name="contract_id" position="after">
                    <label for="date_from_overtime" string="Overtime Period"/>
                    <div>
                        <field name="date_from_overtime" class="oe_inline"/> - <field name="date_to_overtime" class="oe_inline"/>
                    </div>
                </field>
            </field>
        </record>

    </data>
</odoo>