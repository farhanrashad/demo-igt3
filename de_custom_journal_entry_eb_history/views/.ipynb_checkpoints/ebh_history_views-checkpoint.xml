      <record id="account_custom_entry_form_view_inherit_ebh" model="ir.ui.view">
            <field name="name">account.custom.entry.form.view.inehrit.ebh</field>
            <field name="model">account.custom.entry</field>
            <field name="inherit_id" ref="de_custom_journal_entry.account_custom_entry_form_view" />
            <field name="arch" type="xml">
                
                <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
    <page string="History" name="studio_page_Y2JHj">
        <field name="custom_entry_eb_history_line" widget="one2many_list">
            <tree string="Custom Entry Line" editable="bottom">
                <field name="company_id" invisible="1" />
                <field name="currency_id" invisible="1" />
                <field name="project_id" />
                <field name="amount" />
            </tree>
        </field>
    </page>
  </xpath>
                
            </field>
    </record>
      
