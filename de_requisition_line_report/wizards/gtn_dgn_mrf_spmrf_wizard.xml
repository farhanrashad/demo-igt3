<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_gtn_dgn_mrf_spmrf_wizard" model="ir.ui.view">
			<field name="name">gtn.dgn.mrf.spmrf.wizard</field>
			<field name="model">gtn.dgn.mrf.spmrf.wizard</field>
			<field name="arch" type="xml">
				<form string="Gtn DGN MRF SPMRF wizard">
				
					<group>
						<field name="src" />
						<field name="dest" />
					</group>
					
				
					<footer>
						<button name="generate_report" type="object" string="Print" class="oe_highlight" />
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_gtn_dgn_mrf_spmrf_wizard" model="ir.actions.act_window">
			<field name="name">Mrf Spmrf Lines Report</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">gtn.dgn.mrf.spmrf.wizard</field>
			<field name="view_id" ref="view_gtn_dgn_mrf_spmrf_wizard" />
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

    </data>
</odoo>
