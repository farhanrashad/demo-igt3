<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <record id="gtn_report_form_inherit" model="ir.ui.view">
        <field name="name">gtn.transfer.report</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[5]" position="after">
                <page name="transfer" string="Transfer">
                    <group>
                        <group>
                            <!-- table 1 left side-->
                            <field name="gtn_number"/>
                            <field name="Project_name"/>
                            <field name="mrf_ref_no"/>
                            <field name="tower_type"/>
                            <field name="site_address"/>
                            <field name="contractor_name"/>
                           <field name="contractor_description"/>
                            <!-- table 2 left-->
                            <field name="serial"/>
                            <field name="product_code"/>
                            <field name="description"/>
                            <!-- table 3 left side-->
                            <field name="for_irrawady_green_tower_ltd"/>
                            <field name="name_of_authorized_person"/>
                            <field name="designation"/>
                            <field name="authorized_signature_date"/>
                            <field name="contact"/>
                            <field name="gate_pass_form_no"/>
                             <!-- table left side  4-->
                            <field name="any_damage"/>

                        </group>
                        <group>
                            <!-- table 1 right side-->
                            <field name="date_of_gtn"/>
                            <field name="igt_site_id"/>
                            <field name="telenor_site_id"/>
                            <field name="tower_height"/>
                            <field name="transporter_name"/>
                            <field name="contact_no"/>
                            <!-- table 2 right-->
                            <field name="material_supplier_name"/>
                            <field name="mrf_qty"/>
                            <field name="quantity"/>
                            <!-- table 3 right-->
                            <field name="for_contractors"/>
                            <field name="name_of_authorized"/>
                            <field name="contact_number"/>
                            <field name="drive_name"/>
                            <field name="contact_contractors"/>
                            <field name="vehicle_no"/>
                            <!-- table left side  4-->
                            <field name="registered_office_address"/>

                        </group>

                    </group>


                </page>
            </xpath>

        </field>
    </record>

</odoo>

