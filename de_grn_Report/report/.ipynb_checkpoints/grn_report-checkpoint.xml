                               <tr>
                                    <td style="border: 1px solid black">
                                        <b>Gate Entry No</b>
                                    </td>
                                    <td style="border: 1px solid black">
                                        <span t-field="docs.gate_entry_no"/>
                                    </td>
                                    <td colspan="10" style="border: 1px solid black ">
                                        <b>Gate Entry Date</b>
                                    </td>
                                    <td colspan="10" style="border: 1px solid black ">
                                        <span t-field="docs.gate_entry_date"/>
                                    </td>
                                    <td style="border: 1px solid black">
                                        <b>ERP GRN No</b>
                                    </td>
                                    <td style="border: 1px solid black">
                                        <span t-field="docs.erp_grn_no"/>
                                    </td>
                                </tr>
                                <!-- row 2 -->
                                <tr>
                                    <td style="border: 1px solid black">
                                        <b>Vendor Supplier Name</b>
                                    </td>
