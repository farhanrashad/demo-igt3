<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <report id="de_grn_report_id"
            string="Grn Report"
            model="stock.picking"
            report_type="qweb-pdf"
            name="De_GRN_Report.grn_report_print"
            file="De_GRN_Report.grn_report_print"
    />

    <template id="grn_report_print">
