       <template id="portal_material_document_form" name="Portal Form">
        <t t-call="portal.portal_layout">
             <t t-set="o_portal_fullwidth_alert" >
                <t t-call="portal.portal_back_in_edit_mode">
                    <t t-set="backend_url" t-value="'/web#model=project.task&amp;id=%s&amp;view_type=form' % (doc_task.id)"/>
                </t>
            </t>
            <div id="optional_placeholder"></div>
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                  <div id="optional_placeholder"></div>
                    <div class="container">
                      <div class="card">
                        <div class="card-header">
                    <div class="row no-gutters">
                        <div class="col-12">
                            <h5 class="d-flex mb-1 mb-md-0 row">
                                <span t-field="doc_task.name" class="col-7 text-truncate"/>
                                <small class="text-muted d-none d-md-inline"> (#<span t-field="doc_task.id"/>)</small>
                                <div class="col-md-3  text-right">
                                    <small class="text-right">Stage:</small>
                                    <span t-field="doc_task.stage_id.name" class=" badge badge-pill badge-info" title="Current stage of this Timeoff"/>
                                </div>


                                    </h5>
                                </div>
                            </div>
                          </div>
                        </div>
                    </div>
                </t>
               <t t-set="card_body">


                        <div class="row">
                                <div class="col-md-6"> <strong>Task Title : </strong>
                                    <t t-if="doc_task.name">
                                        <span t-esc="doc_task.name"/>
                                    </t>
                                </div>


                        </div>

                        <br/><br/>




                   <div class="row">

                        <div class="offset-md-10 col-md-1">
                            <a t-attf-href="/task/submit/#{doc_task.id}?"><i  class="btn btn-primary">Submit</i></a>
                        </div>
                        <div class="col-md-1">
                        </div>


                    </div>
                </t>
            </t>

            <div class="mt32">
                <h4><strong>Message and communication history</strong></h4>
                <t t-call="portal.message_thread">
                    <t t-set="object" t-value="doc_task"/>
                    <t t-set="pid" t-value="pid"/>
                    <t t-set="hash" t-value="hash"/>
                </t>
            </div>
        </t>
    </template>






 <template id="de_project_material_requisition_import.material_attachment" name="Material Attachment">
        <t t-call="portal.portal_layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div class="container">
                    <div class="row">

                        <t t-if="task">
                            <h1 class="text-center">Add Attachment Click On link <a t-if="task" t-attf-href="/material/task/#{task.id}">#<span t-field="task.id"/></a></h1>
                        </t>
                    </div>
                </div>
            </div>
         </t>
    </template>



 <template id="de_project_material_requisition_import.material_submited" name="Material Request">
        <t t-call="portal.portal_layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div class="container">
                    <div class="row">
                        <t t-if="request.session.get('form_builder_model_model', '') == 'project.task'">
                            <t t-set="task_docs" t-value="request.portal._website_form_last_record().sudo()"/>
                        </t>
                        <h1 class="text-center"><i class="fa fa-check-circle fa-2x text-success" role="img" aria-label="Success" title="Success"/></h1>
                        <t t-if="advance">
                            <h1 class="text-center">Your Material Request Number is <a t-if="request.session.uid" t-attf-href="/advance/salary/page/#{advance.id}">#<span t-field="advance.id"/></a><t t-if="not request.session.get('uid')">#<span t-field="advance.id"/></t>.</h1>
                        </t>
                        <h1 class="text-center">Thank you for submitting your Material Documents!</h1>
                    </div>
                </div>
            </div>
         </t>
    </template>











