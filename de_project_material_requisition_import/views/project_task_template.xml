    <template id="portal_material_document_layout" name="Portal layout: Account menu material DOCUMENT" inherit_id="portal.portal_breadcrumbs" priority="20">
        <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
            <li t-if="page_name == 'doc_task' or doc_task" class="col-lg-2" t-attf-class="breadcrumb-item #{'active ' if not doc_task else ''}">
                <a t-if="doc_task" t-attf-href="/material/document?{{ keep_query() }}">Documents Submit</a>
                <t t-else="">Material Documents</t>
            </li>
            <li t-if="doc_task" class="breadcrumb-item active text-truncate col-8 col-lg-10">
                <t t-esc="doc_task.name"/>
            </li>
        </xpath>
    </template>





