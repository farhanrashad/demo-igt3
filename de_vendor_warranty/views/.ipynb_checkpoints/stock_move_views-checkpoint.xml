<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_move_line_operation_tree_inheirt_warranty" model="ir.ui.view">
        <field name="name">view.stock.move.line.operation.tree.inherit.warranty</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
        <field name="arch" type="xml">
            <field name="lot_id" position="after">
                <field name="allow_warranty" invisible="1"/>
                <field name="warranty_date_start" attrs="{'invisible':[('allow_warranty','=',False)], 'required':[('allow_warranty','!=',False)]}"/>
                <field name="warranty_date_end" attrs="{'invisible':[('allow_warranty','=',False)], 'required':[('allow_warranty','!=',False)]}"/>
                <field name="warranty_days" attrs="{'invisible':[('allow_warranty','=',False)], 'required':[('allow_warranty','!=',False)]}"/>
            </field>
        </field>
      </record>
    
    
</odoo>